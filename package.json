{
  "name": "server",
  "version": "1.0.4",
  "description": "server ",
  "main": "app.js",
  "scripts": {
    "prestart": "rm -rf node_modules && npm install",
    "start": "npm run build && npm run start:prod",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts node_modules/.bin/bunyan",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run copy:ejs",
    "start:prod": "node build/src/app"
  },
  "author": "flower lewyn cordoba moreno",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.721.0",
    "@aws-sdk/client-s3": "^3.722.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.2.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/bunyan": "^1.8.10",
    "@types/compression": "^1.7.4",
    "@types/cookie-session": "^2.0.46",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/hpp": "^0.2.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.14.200",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "ncp": "^2.0.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0"
  }
}
